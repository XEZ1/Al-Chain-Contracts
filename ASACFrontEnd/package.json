{
  "name": "asacfrontend",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "cross-env BABEL_ENV=test BACKEND_URL=https://example.com jest",
    "test:watch": "cross-env BABEL_ENV=test BACKEND_URL=https://example.com jest --watch",
    "test:coverage": "cross-env BABEL_ENV=test BACKEND_URL=https://example.com jest --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/__tests__/**",
      "!**/@(tests|test)/**",
      "!<rootDir>/path/to/exclusions/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-navigation|@react-navigation.*|expo(nent)?|@expo(nent)?/.*)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.18",
    "deprecated-react-native-prop-types": "^5.0.0",
    "expo": "^51.0.5",
    "expo-clipboard": "~6.0.3",
    "expo-document-picker": "~12.0.1",
    "expo-notifications": "~0.28.1",
    "expo-secure-store": "~13.0.1",
    "expo-sharing": "~12.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.74.1",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-webview": "13.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.5.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.1",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^18.2.0"
  },
  "private": true
}
